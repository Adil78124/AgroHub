    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.4.2",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
